<article data-type="kunta-api-service-location-component" data-component="description" data-service-channel-id="{{ serviceLocationChannel.id }}" data-lang="{{ lang }}">{% include 'service-location-components/description.twig' %}</article>
<article data-type="kunta-api-service-location-component" data-component="accessibility" data-service-channel-id="{{ serviceLocationChannel.id }}" data-lang="{{ lang }}">{% include 'service-location-components/accessibility.twig' %}</article>
<aside class="kunta-api-aside mceNonEditable" contenteditable="false" readonly="true">
  <div class="kunta-api-aside-contents mceEditable" contenteditable="true" readonly="false">
    <h2>Yhteystiedot</h2>
    {% spaceless %}
      <article data-type="kunta-api-service-location-component" data-component="email" data-service-channel-id="{{ serviceLocationChannel.id }}" data-lang="{{ lang }}">{% include 'service-location-components/email.twig' %}</article>    
      <article data-type="kunta-api-service-location-component" data-component="phone" data-service-channel-id="{{ serviceLocationChannel.id }}" data-lang="{{ lang }}">{% include 'service-location-components/phone.twig' %}</article>
      <article data-type="kunta-api-service-location-component" data-component="phone-charge-info" data-service-channel-id="{{ serviceLocationChannel.id }}" data-lang="{{ lang }}">{% include 'service-location-components/phone-charge-info.twig' %}</article>
      <article data-type="kunta-api-service-location-component" data-component="fax" data-service-channel-id="{{ serviceLocationChannel.id }}" data-lang="{{ lang }}">{% include 'service-location-components/fax.twig' %}</article>
      <article data-type="kunta-api-service-location-component" data-component="addresses" data-service-channel-id="{{ serviceLocationChannel.id }}" data-lang="{{ lang }}">{% include 'service-location-components/addresses.twig' %}</article>
      <article data-type="kunta-api-service-location-component" data-component="servicehours" data-service-channel-id="{{ serviceLocationChannel.id }}" data-lang="{{ lang }}">{% include 'service-location-components/servicehours.twig' %}</article>  
    {% endspaceless %}
  </div>
</aside>
